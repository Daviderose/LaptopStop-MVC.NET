@model LaptopStop_MVC.ViewModels.AddCategoryViewModel

<h1>Add Category</h1>

<form class="form-group" asp-controller="Category" asp-action="Add" method="post">
    <label asp-for="Name"></label>
    <input class="form-control" asp-for="Name" />
    <span asp-validation-for="Name"></span>

    <input type="submit" value="Add Category" />
</form>
